<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <constant name="struts.enable.DynamicMethodInvocation" value="ture" />
    <constant name="struts.devMode" value="true" />

    <package name="default" namespace="/" extends="struts-default">

      
        <global-results>
            <result name="error">/WEB-INF/jsp/error.jsp</result>
        </global-results>

        <global-exception-mappings>
            <exception-mapping exception="java.lang.Exception" result="error"/>
        </global-exception-mappings>

       
    </package>
    <package name="userManager" extends="default">
      <action name="*User" class="com.qhit.lh.g4.hsj.sbm.user.action.UserAction">
        <result name="loginSuccess">/jsp/admin_index.jsp</result>
        <result name="loginFail">/index.jsp</result>
        <result name="input">/index.jsp</result>
        <result name="listSuccess">/jsp/userAdmin.jsp</result>
        <result name="uploadSuccess" type="chain">addUser</result>
        <result name="addSuccess" type="redirectAction">getAllUser</result>
      </action>
    </package>
   
</struts>
